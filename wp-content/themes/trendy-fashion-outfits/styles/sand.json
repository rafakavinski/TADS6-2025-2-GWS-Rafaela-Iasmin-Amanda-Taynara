        { "color": "#b08968", "name": "Secondary", "slug": "secondary" },
        { "color": "#fffaf0", "name": "Background", "slug": "background" },
        { "color": "#5c5c5c", "name": "Body Text", "slug": "body-text" },
        { "color": "#f1e0c6", "name": "Secondary Background", "slug": "background-secondary" },
        { "color": "#e6ccb2", "name": "Soft Sand", "slug": "soft-sand" },
        { "color": "#ede0d4", "name": "Desert White", "slug": "desert-white" }
